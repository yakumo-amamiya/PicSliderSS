<UserControl x:Class="PicSliderSS.PicSliderScreen.PicSliderScreenA"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:PicSliderSS.PicSliderScreen"
             xmlns:psg="clr-namespace:PicSliderSS.PicSliderGrid"
             xmlns:sys="clr-namespace:System;assembly=mscorlib"
             mc:Ignorable="d">
    <UserControl.Resources>
        <sys:Double x:Key="widthA">1920</sys:Double>
        <sys:Double x:Key="heightA">1080</sys:Double>
    </UserControl.Resources>
    <Grid>
        <psg:PicSliderGridLeft
            x:Name="PicSliderGrid1"
            Width="{DynamicResource widthA}" 
            Height="{DynamicResource heightA}"/>
    </Grid>
</UserControl>
